{
  "id": "gmail-registration-template",
  "name": "Gmail注册流程",
  "domain": "accounts.google.com",
  "description": "适用于Google账号注册的完整流程，包含邮箱验证和人机验证处理",
  "steps": [
    {
      "id": "step-1",
      "type": "fillInput",
      "name": "填入名字",
      "description": "在名字输入框中填入随机生成的名字",
      "selector": "input[name='firstName']",
      "value": "Test",
      "options": {
        "delay": 500,
        "clearFirst": true,
        "waitForElement": 3000,
        "scrollIntoView": true
      }
    },
    {
      "id": "step-2",
      "type": "fillInput",
      "name": "填入姓氏",
      "description": "在姓氏输入框中填入随机生成的姓氏",
      "selector": "input[name='lastName']",
      "value": "User",
      "options": {
        "delay": 500,
        "clearFirst": true,
        "waitForElement": 3000,
        "scrollIntoView": true
      }
    },
    {
      "id": "step-3",
      "type": "fillInput",
      "name": "填入用户名",
      "description": "在用户名输入框中填入生成的邮箱地址",
      "selector": "input[name='Username']",
      "value": "{{email}}",
      "options": {
        "delay": 800,
        "clearFirst": true,
        "waitForElement": 3000,
        "scrollIntoView": true
      }
    },
    {
      "id": "step-4",
      "type": "fillInput",
      "name": "填入密码",
      "description": "在密码输入框中填入随机生成的密码",
      "selector": "input[name='Passwd']",
      "value": "TestPassword123!",
      "options": {
        "delay": 500,
        "clearFirst": true,
        "waitForElement": 3000,
        "scrollIntoView": true
      }
    },
    {
      "id": "step-5",
      "type": "fillInput",
      "name": "确认密码",
      "description": "在确认密码输入框中填入相同的密码",
      "selector": "input[name='ConfirmPasswd']",
      "value": "TestPassword123!",
      "options": {
        "delay": 500,
        "clearFirst": true,
        "waitForElement": 3000,
        "scrollIntoView": true
      }
    },
    {
      "id": "step-6",
      "type": "clickButton",
      "name": "点击下一步",
      "description": "点击下一步按钮继续注册流程",
      "selector": "#accountDetailsNext",
      "options": {
        "delay": 1000,
        "waitForElement": 2000,
        "scrollIntoView": true
      }
    },
    {
      "id": "step-7",
      "type": "humanVerification",
      "name": "完成人机验证",
      "description": "等待用户完成Google的人机验证（reCAPTCHA等）",
      "options": {
        "timeout": 180000,
        "skipable": false,
        "autoDetect": true,
        "retryable": true
      }
    },
    {
      "id": "step-8",
      "type": "waitForElement",
      "name": "等待手机号页面",
      "description": "等待手机号验证页面加载",
      "selector": "input[type='tel']",
      "options": {
        "timeout": 10000,
        "checkInterval": 500
      }
    },
    {
      "id": "step-9",
      "type": "fillInput",
      "name": "填入手机号",
      "description": "填入手机号码（可选步骤）",
      "selector": "input[type='tel']",
      "value": "",
      "options": {
        "delay": 500,
        "clearFirst": true,
        "waitForElement": 3000,
        "scrollIntoView": true
      }
    },
    {
      "id": "step-10",
      "type": "clickButton",
      "name": "跳过手机验证",
      "description": "点击跳过手机验证（如果可用）",
      "selector": "button[data-action='skip']",
      "options": {
        "delay": 1000,
        "waitForElement": 2000,
        "scrollIntoView": true
      }
    },
    {
      "id": "step-11",
      "type": "waitForElement",
      "name": "等待成功页面",
      "description": "等待账号创建成功页面",
      "selector": ".welcome-page, .account-created",
      "options": {
        "timeout": 15000,
        "checkInterval": 1000
      }
    }
  ],
  "variables": {
    "email": "{{email}}"
  },
  "globalOptions": {
    "maxRetries": 3,
    "retryInterval": 2000,
    "pageLoadTimeout": 15000,
    "elementTimeout": 8000
  },
  "enabled": true,
  "createdAt": 1704067200000,
  "updatedAt": 1704067200000
}
