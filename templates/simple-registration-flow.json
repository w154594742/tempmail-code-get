{
  "id": "simple-registration-template",
  "name": "简单注册流程",
  "domain": "*",
  "description": "适用于简单注册表单的基础流程，包含邮箱填入和基本验证",
  "steps": [
    {
      "id": "step-1",
      "type": "fillInput",
      "name": "填入邮箱",
      "description": "在邮箱输入框中填入生成的临时邮箱",
      "selector": "input[type='email']",
      "value": "{{email}}",
      "options": {
        "delay": 500,
        "clearFirst": true,
        "waitForElement": 3000,
        "scrollIntoView": true
      }
    },
    {
      "id": "step-2",
      "type": "humanVerification",
      "name": "完成验证",
      "description": "完成人机验证（如果需要）",
      "options": {
        "timeout": 60000,
        "skipable": true,
        "autoDetect": true,
        "retryable": true
      }
    },
    {
      "id": "step-3",
      "type": "clickButton",
      "name": "提交表单",
      "description": "点击提交按钮",
      "selector": "button[type='submit'], input[type='submit']",
      "options": {
        "delay": 1000,
        "waitForElement": 2000,
        "scrollIntoView": true
      }
    },
    {
      "id": "step-4",
      "type": "waitForElement",
      "name": "等待成功提示",
      "description": "等待提交成功的提示信息",
      "selector": ".success-message, .thank-you, .confirmation",
      "options": {
        "timeout": 10000,
        "checkInterval": 500
      }
    }
  ],
  "variables": {
    "email": "{{generated_email}}"
  },
  "globalOptions": {
    "maxRetries": 2,
    "retryInterval": 1500,
    "pageLoadTimeout": 8000,
    "elementTimeout": 4000
  },
  "enabled": true,
  "createdAt": 1704067200000,
  "updatedAt": 1704067200000
}
